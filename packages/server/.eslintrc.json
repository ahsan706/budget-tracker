{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "google",
        "prettier",
        "prettier/prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "quotes": [2, "single", { "avoidEscape": true }]
    }
}
